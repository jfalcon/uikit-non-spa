{
  "author": "Jeremy Falcon",
  "description": "Boilerplate Non-SPA Site using React and UIkit",
  "homepage": "https://github.com/jfalcon/uikit-non-spa#readme",
  "license": "MIT",
  "name": "uikit-non-spa",
  "version": "0.0.1",
  "bugs": {
    "url": "https://github.com/jfalcon/uikit-non-spa/issues"
  },
  "browserslist": [
    "Chrome >= 45",
    "Firefox >= 38",
    "Edge >= 12",
    "Explorer >= 10",
    "iOS >= 9",
    "Safari >= 9",
    "Android >= 4.4",
    "Opera >= 30"
  ],
  "config": {
    "defaults": {
      "filename": "index"
    },
    "directories": {
      "modules": "node_modules",
      "output": ".dist",
      "pages": "pages",
      "source": "site"
    }
  },
  "keywords": [
    "uikit",
    "react",
    "boilerplate",
    "es6",
    "es2015",
    "sass",
    "scss"
  ],
  "scripts": {
    "prebuild": "npm-run-all --silent clear clean",
    "build": "cross-env NODE_ENV=production babel-node ./tools/build.js",
    "clean": "rimraf ./.dist",
    "clear": "babel-node ./tools/clear.js",
    "serve": "cross-env NODE_ENV=production babel-node ./tools/serve.js",
    "serve:watch": "babel-node ./tools/serve.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jfalcon/uikit-non-spa.git"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "flexibility": "^2.0.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^3.1.3",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "sprintf-js": "^1.1.1",
    "uikit": "^3.0.0-beta.35",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.4",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.1.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-conditional-compilation": "0.0.1",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-react-display-name": "^2.0.0",
    "babel-plugin-syntax-async-generators": "^6.13.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.26.0",
    "colors": "^1.1.2",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.8",
    "eslint": "^4.14.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-watch": "^3.1.3",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "isomorphic-fetch": "^2.2.1",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "open": "0.0.5",
    "postcss-flexbugs-fixes": "^3.2.0",
    "postcss-flexibility": "^2.0.0",
    "postcss-loader": "^2.0.10",
    "quickly-copy-file": "^1.0.0",
    "redux-immutable-state-invariant": "^2.1.0",
    "resolve-url-loader": "^2.2.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "stylelint": "^8.4.0",
    "stylelint-webpack-plugin": "^0.9.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^2.0.3",
    "webpack-hot-middleware": "^2.21.0"
  }
}
